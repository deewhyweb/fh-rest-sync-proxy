{
  "name": "fh-rest-sync-proxy",
  "version": "1.0.0",
  "description": "Proxy $fh.sync calls from a client to an MBaaS Service that exposes a RESTful API",
  "main": "lib/sync.js",
  "scripts": {
    "format": "prettier --write --no-semi --single-quote lib/*.js test/*.js",
    "check-coverage": "istanbul check-coverage --statements 98 --branches 100 --functions 100 --lines 98",
    "cover": "istanbul cover -x *.test.js _mocha test/",
    "precommit": "npm run format && git add .",
    "unit": "mocha test/",
    "test": "npm run unit && npm run cover && npm run check-coverage"
  },
  "author": {
    "name": "Evan Shortiss"
  },
  "contributors": [
    {
      "name": "Jim Dillon"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "fh-bunyan": "~0.3.0",
    "url-join": "~1.1.0",
    "verror": "~1.10.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "fh-mbaas-api": "~9.1.4",
    "husky": "~1.1.4",
    "istanbul": "~0.4.5",
    "jshint": "^2.9.2",
    "mocha": "~2.5.3",
    "prettier": "~1.15.2",
    "proxyquire": "~1.8.0",
    "sinon": "^1.17.4"
  },
  "peerDependencies": {
    "fh-mbaas-api": ">=7.0.0"
  }
}
